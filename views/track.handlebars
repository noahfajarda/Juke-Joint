<!-- album -->
<h1>Album:</h1>
<h1>{{trackName}}</h1>
<h1>{{trackArtist}}</h1>
<iframe
    style="border-radius: 12px"
    src="https://open.spotify.com/embed/track/{{trackId}}?utm_source=generator"
    width="100%"
    height="380"
    frameborder="0"
    allowfullscreen=""
    allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture"
    loading="lazy"
></iframe>
<h1>track ID used: {{trackId}}</h1>
<h1>{{trackLyrics}}</h1>